#while swapped

indexList=[5,2,1,3,6,4]
def Bubbles(indexList):
  for i in range(len(indexList)-1,0,-1):
    for j in range(0,i):
      if indexList[j]>indexList[j+1]:
        indexList[j],indexList[j+1]=indexList[j+1],indexList[j]
  return indexList
  
  
print(Bubbles(indexList))

